{
  "name": "Hubot",
  "description": "Hubot on Slack", "repository": "https://github.com/Tomohiro/hubot",
  "keywords": ["node.js", "hubot", "slack"],
  "website": "http://twitterokinawa-hubot.herokuapp.com",
  "env": {
    "TZ": {
      "descirption": "Timezone",
      "value": "Asia/Tokyo"
    },
    "HUBOT_SLACK_TOKEN": {
      "description": "Slack API token",
      "value": ""
    },
    "HUBOT_DEPLOY_APPS_JSON": {
      "description": "Application configrations to hubot-deploy",
      "value": "deploy.json"
    },
    "HUBOT_GITHUB_TOKEN": {
      "description": "GitHub User Token to GitHub deployment API",
      "value": "c397f6c51ae25d057a8d2c8335ea06c0e09094d3"
    },
    "HUBOT_ANIMEMAP_PREF": {
      "description": "Anime broadcasting area in Japan to hubot-animemap",
      "value": "tokyo"
    },
    "HUBOT_NULLPO_RESPONSE_STYLE": {
      "description": "NullPointerException joke configuration to hubot-nullpo ('rich' means using AA)",
      "value": "rich"
    },
    "HUBOT_ZOI_SOURCE_URL": {
      "description": "Zoi source url to hubot-zoi",
      "value": ""
    },
    "HUBOT_TWITTER_ACCESS_TOKEN": {
      "description": "Twitter access token to hubot-tweets",
      "value": ""
    },
    "HUBOT_TWITTER_ACCESS_TOKEN_SECRET": {
      "description": "Twitter access token secret to hubot-tweets",
      "value": ""
    },
    "HUBOT_TWITTER_CONSUMER_KEY": {
      "description": "Twitter consumer key to hubot-tweets",
      "value": ""
    },
    "HUBOT_TWITTER_CONSUMER_SECRET": {
      "description": "Twitter consumer secret to hubot-tweets",
      "value": ""
    },
    "addons": ["rediscloud"]
  }
}
